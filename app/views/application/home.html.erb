<h1>Request a shared ride</h1>

<%= form_for @new_user do |f| %>
  <%= f.label :phone_number %>:
  <%= f.text_field :phone_number %><br />

  <%= f.label :start_location %>:
  <%= f.text_field :start_location %><br />

  <%= f.label :end_location %>:
  <%= f.text_field :end_location %><br />

  <%= f.submit %>
<% end %>